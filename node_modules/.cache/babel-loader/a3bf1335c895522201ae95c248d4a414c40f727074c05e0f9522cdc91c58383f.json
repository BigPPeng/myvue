og(row)\n      const userName = row.username;\n      const confirmResult = await this.$confirm('是否确认删除名称为\"' + userName + '\"的用户?', \"提示\", {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).catch(err => err)\n      // 成功删除为confirm 取消为 cancel\n      if (confirmResult != 'confirm') {\n        this.$refs.multipleTable.clearSelection();\n        return this.$message.info(\"已取消删除\");\n      }\n      const { data: bb } = await this.$http.post(\"/api/user/deleteUser\", { id: row.id });\n      this.$message.success(bb);\n      this.getList();\n    },\n    //取消\n    reset() {\n      this.queryForm.userType = null,\n      this.getList();\n    }\n  }\n  \n  };\n  </script>\n  \n  <style lang=\"less\" scoped>\n  .el-breadcrumb {\n    margin-bottom: 15px;\n    font-size: 17px;\n  }\n  </style>"],"mappings":"AAqDA;EACAA,KAAA;IACA;MACAC,SAAA;IACA;EACA;EACAC,OAAA;IACA;IACA,MAAAC,QAAA;MACA,IAAAC,IAAA,GAAAC,MAAA,CAAAC,cAAA,CAAAC,OAAA;MACA,IAAAC,OAAA,GAAAC,IAAA,CAAAC,KAAA,CAAAN,IAAA;MACA;QAAAJ,IAAA,EAAAW;MAAA,eAAAC,KAAA,CAAAC,IAAA;QAAAC,EAAA,EAAAN,OAAA,CAAAM;MAAA;MACAC,OAAA,CAAAC,GAAA,CAAAL,EAAA;MACA,KAAAV,SAAA,GAAAU,EAAA,CAAAX,IAAA;IACA;IACA;IACA,MAAAiB,WAAAC,GAAA;MACAH,OAAA,CAAAC,GAAA,CAAAE,GAAA;MACA,MAAAC,QAAA,GAAAD,GAAA,CAAAE,QAAA;MACA,MAAAC,aAAA,cAAAC,QAAA,gBAAAH,QAAA;QACAI,iBAAA;QACAC,gBAAA;QACAC,IAAA;MACA,GAAAC,KAAA,CAAAC,GAAA,IAAAA,GAAA;MACA;MACA,IAAAN,aAAA;QACA,KAAAO,KAAA,CAAAC,aAAA,CAAAC,cAAA;QACA,YAAAC,QAAA,CAAAC,IAAA;MACA;MACA;QAAAhC,IAAA,EAAAiC;MAAA,eAAArB,KAAA,CAAAC,IAAA;QAAAC,EAAA,EAAAI,GAAA,CAAAJ;MAAA;MACA,KAAAiB,QAAA,CAAAG,OAAA,CAAAD,EAAA;MACA,KAAA9B,OAAA;IACA;IACA;IACAgC,MAAA;MACA,KAAAC,SAAA,CAAAC,QAAA,SACA,KAAAlC,OAAA;IACA;EACA;AAEA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}